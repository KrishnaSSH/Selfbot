{
  "env": {
    "node": true,
    "es2022": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": [
      "warn",
      { "args": "after-used", "ignoreRestSiblings": true }
    ],
    "no-console": "off"
  }
}
